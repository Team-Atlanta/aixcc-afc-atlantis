from typing import Optional

class PyTestLangWarning:
    def __init__(self, kind: str, message: str, record: Optional[str] = None, field: Optional[str] = None) -> None: ...

    @property
    def kind(self) -> str: ...

    @property
    def message(self) -> str: ...

    @property
    def record(self) -> Optional[str]: ...

    @property
    def field(self) -> Optional[str]: ...

def schema() -> str: ...
def validate(testlang: str, python_codes: set[str]) -> list[PyTestLangWarning]: ...
def normalize(testlang: str) -> str: ...
def hash(testlang: str) -> int: ...
def update(
    testlang: str,
    partial_testlang: str,
    records_to_remove: set[str],
) -> str: ...
def visualize(testlang: str, python_codes: dict[str, str], output_path: str) -> None: ...
